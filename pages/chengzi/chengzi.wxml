<!--pages/chengzi/chengzi.wxml-->
<view class="you-zi">
    <image class="bg-img" src="../../images/yangtao/bg-k.png" mode="widthFix"></image>
    <view class="yun-ceng">
        <image class="yun-max" src="../../images/yangtao/baiyun2-dong.png"></image>
        <image class="yun-min" src="../../images/yangtao/baoyun-dong.png"></image>
    </view>
    <view class="content">
        <image bind:tap="goToMall" style="width:238rpx;position:absolute;right:-60rpx;top:40vh;transform: translateY(-50%)" mode="widthFix" src="../../images/yangtao/shangcheng.png"></image>
        <!--果树部分-->
        <view style="width: 490rpx;height: 190rpx;position: absolute;top: 50%;left: 50%;transform: translate(-50%,75%)">
            <image style="width: 100%" src="../../images/yangtao/zhonghziqu.png" mode="widthFix"></image>
            <image style="width:402rpx;height:102rpx;position: absolute;left:44rpx;top:-102rpx" src="../../images/yangtao/weilan.png"></image>
            <image bind:tap="clickShu" class="{{shuClassName}}" style="position: absolute;bottom: 80rpx;width: 100%;left: 0%;" mode="widthFix" src="../../images/yangtao/{{notChargeOrangeAmount?'2':'1'}}-{{treeRank===1?treeRank:treeRank-1}}.png"></image>
            <view style="text-align: center;width: 138rpx;position: absolute;bottom:95rpx;left: 13rpx;transform:rotateZ(-5deg);line-height: 26rpx;">
                <image style="width:138rpx;height:134rpx;" src="../../images/yangtao/wff.png"></image>
                <view style="position:absolute;top:7rpx;text-align: center;width:100%">
                    <view class="muban-text font-20 font-bold color-ffffff">吴菲菲</view>
                    <view class="muban-text font-20 font-bold color-ffffff">的杨桃果园</view>
                </view>
            </view>
            <image wx:for="{{feiLiaoBing}}" wx:key="index" class="{{item}}" style="width: 50rpx;height: 50rpx;position: absolute;bottom: 140rpx;right: 120rpx;" mode="widthFix" src="../../images/chengzi/feiliaoli.png"></image>
            <view class="{{feiLiaoDaiClass}}" bind:tap="playShouFeiLiao" style="position: absolute;bottom: 80rpx;width: 125rpx;height:125rpx;right: 12rpx;">
                <image style="width: 100%" mode="widthFix" src="../../images/yangtao/feiliao.png"></image>
                <view style="position: absolute;width: 100%;bottom: 14rpx;text-align: center;line-height: 30rpx;">
                    <text class="font-20 color-8F491B">
                        肥料
                    </text>
                    <text class="font-30 font-bold color-8F491B">900</text>
                    <image class="can-Ling-qu" src="../../images/yangtao/qipao.png" mode="widthFix"></image>
                </view>
            </view>
            <view style="position:absolute;bottom:-100rpx;left:50%;transform:translate(-50%,0);width: 300rpx;height: 88rpx;text-align:center;">
                <view style="width: 300rpx;border: 3rpx solid #FDE6A2;border-radius: 18rpx;background-color: #00000000;padding: 3rpx;">
                    <view style="width: {{progressWidth}}%" class="progress"></view>
                </view>
                <text wx:if="{{treeRank<=10}}" style="white-space: nowrap;" class="font-24 color-ffffff">再施肥{{muckAmount}}g{{treeName}}</text>
                <text wx:else style="white-space: nowrap;" class="font-24 color-ffffff">您已经是最高级了</text>
            </view>
            <view class="{{shiFeiBtnClass}}" bind:tap="" style="position: absolute;bottom: -18vh;left:62rpx;width: 365rpx;height:125rpx;">
                <image wx:if="{{shiFeiGifShow}}" style="width: 100%;position: absolute;bottom: 80rpx;left: 0;" mode="widthFix" src="../../images/chengzi/shifei.gif"></image>
                <image style="width: 100%" mode="widthFix" src="../../images/yangtao/shifei.png"></image>
                <view bind:tap="clickShiFeiBtn" style="position: absolute;top:0;left:0;width: 100%;text-align: center;padding-top: 22rpx;line-height: 40rpx;">
                    <view class="font-36 font-bold color-7A5A45">施肥</view>
                    <view class="font-26 color-94734C">我的肥料 20</view>
                </view>
            </view>
            <view bind:tap="playShouHuo" class="font-20 color-ffffff can-zhai-qu" style="background: url('../../images/yangtao/qipaobg.png') 0 0/100%">
                <view class="flex-view" style="justify-content: center">
<!--                    <image style="width: 24rpx;height: 24rpx;" src="../../images/chengzi/icon-1.png"></image>-->
                    <view class="font-26 font-bold color-ffffff kezhaiqu-qipao-text" style="padding: 0 14rpx">可摘取</view>
<!--                    <image style="width: 24rpx;height: 24rpx;" src="../../images/chengzi/icon-1.png"></image>-->
                </view>
                <view class="font-24 font-bold color-ffffff kezhaiqu-qipao-text">2.0000个橙子</view>
            </view>
            <image  wx:for="{{shouHuoList}}" wx:key="index" class="{{item}}" style="width: 50rpx;height: 50rpx;position: absolute;bottom: 500rpx;left: 300rpx;" mode="widthFix" src="../../images/chengzi/icon-1.png"></image>
        </view>
        <!--橙子池子-->
        <view style="position: absolute;top:10vh;left:50%;transform:translate(-50%,0);text-align: center;">
            <view style="width: 500rpx;height: 250rpx;position: relative;transform: translate(0 , -30rpx)">
                <image style="width: 500rpx;height: 250rpx;" mode="widthFix" src="../../images/yangtao/yangtaoguoyuanzhutu.png"></image>
                <view style="position: absolute;bottom:40rpx;left:0;width: 100%;text-align: center;padding-top: 38rpx;line-height: 46rpx">
                    <view class="font-34 font-bold color-ffffff leyuan-title1">今日杨桃数</view>
                    <view class="font-34 font-bold color-ffffff leyuan-title1">{{todayNumber}}个</view>
                </view>
                <view style="position: absolute;bottom:-20rpx;width:100%" class="muban-text font-28 font-bold color-ffffff">施肥越多获得的杨桃越多</view>
            </view>
        </view>
<!--        <view class="right-list flex-view" style="top: 170rpx;">-->
<!--            <view class="{{shouHuoBtnClass}}" bind:tap="openShouHuo">-->
<!--                <image src="../../images/chengzi/icon-shouhuo.png" mode="widthFix"></image>-->
<!--                <text class="font-26 right-list-text1">收获</text>-->
<!--            </view>-->
<!--            <view bind:tap="goToRaiders">-->
<!--                <image src="../../images/chengzi/icon-gonglue.png" mode="widthFix"></image>-->
<!--                <text class="font-26 right-list-text2">攻略</text>-->
<!--            </view>-->
<!--            <view bind:tap="goToMall">-->
<!--                <image src="../../images/chengzi/icon-shangcheng.png" mode="widthFix"></image>-->
<!--                <text class="font-26 right-list-text3">商城</text>-->
<!--            </view>-->
<!--        </view>-->
        <view class="right-list flex-view" style="bottom: 150rpx;">
            <view class="{{shouHuoBtnClass}}" bind:tap="openShouHuo">
                <image src="../../images/yangtao/btn-shouhuo.png" mode="widthFix"></image>
                <text class="font-26 right-list-text1">收获</text>
            </view>
            <view bind:tap="openTouFeiLiao">
                <image class="guang-huan" src="../../images/chengzi/guanghuan.png" mode="widthFix"></image>
                <image src="../../images/yangtao/btn-zhaofeiliao.png" mode="widthFix"></image>
                <text class="font-26 right-list-text2">偷肥料</text>
                <view class="font-20 color-ffffff red-tips" style="background: url('../../images/chengzi/redTips.png') 0 0/100%">可偷</view>
            </view>
            <view bind:tap="openDaoJu">
                <image src="../../images/yangtao/btn-daoju.png" mode="widthFix"></image>
                <text class="font-26 right-list-text3">道具</text>
            </view>
            <view bind:tap="openJiFeiLiao">
                <image src="../../images/yangtao/btn-jifenliao.png" mode="widthFix"></image>
                <text class="font-26 right-list-text4">集肥料</text>
            </view>
        </view>
<!--    下方滑出弹框-->
        <!--偷肥料-->
        <view class="bottom-window-shadow" style="{{touFeiLiaoShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window">
                <view class="bottom-window-title">
                    <image class="bottom-window-title-icon" src="../../images/chengzi/touFeiLiao-title.png" mode="widthFix"></image>
                    <image class="bottom-window-title-bg" src="../../images/chengzi/title1.png" mode="widthFix"></image>
                    <text class="bottom-window-title-text shouhuo-title font-40 font-bold color-ffffff">偷肥料</text>
                    <image class="bottom-window-title-close" bind:tap="closeTouFeiLiao" src="../../images/chengzi/icon-close.png" mode="widthFix"></image>
                </view>
                <view class="bottom-window-content">
                    <view class="bottom-window-content-list" style="padding: 0">
                        <view class="flex-view" style="background: #FFFAEA;border-radius: 30px 30px 0px 0px;padding: 25rpx;justify-content: space-between;">
                            <view class="flex-view">
                                <image style="width: 106rpx;height: 106rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="../../images/chengzi/icon-shentoushoutao1.png"></image>
                                <view>
                                    <view class="font-36 color-FE963A">获得橙子</view>
                                    <view class="font-24 color-C0C0C0">购买神投手套自动偷</view>
                                </view>
                            </view>
                            <image bind:tap="clickGoBuy" src="../../images/chengzi/gobuy.png" style="width: 173rpx;height: 81rpx"></image>
                        </view>
                        <scroll-view bindscrolltolower="getCanStealList" style="height: 50vh;padding: 0 25rpx;box-sizing: border-box;" scroll-y="true">
                            <view wx:for="{{canStealList}}" wx:key="index" class="flex-view" style="padding: 20rpx 0;border-bottom: 1rpx solid #EBE7E7;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="{{item.avatar}}"></image>
                                    <view>
                                        <view class="font-30 color-302424">{{item.nickName}}</view>
                                        <view class="font-26 color-C0C0C0">{{item.phone}}</view>
                                    </view>
                                </view>
                                <image data-id="{{item.stealUserId}}" src="../../images/chengzi/icon-shou.png" style="width: 80rpx;height: 84rpx"></image>
                            </view>
                        </scroll-view>
                    </view>
                </view>
            </view>
        </view>
        <!--道具-->
        <view class="bottom-window-shadow" style="{{daoJuShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window">
                <view class="bottom-window-title" style="height: 156rpx">
                    <image class="bottom-window-title-icon" src="../../images/chengzi/daoju-title.png" mode="widthFix"></image>
                    <image class="bottom-window-title-bg" style="height: 156rpx" src="../../images/chengzi/daoju-title1.png" mode="widthFix"></image>
                    <text class="bottom-window-title-text shouhuo-title font-40 font-bold color-ffffff">道具</text>
                    <image class="bottom-window-title-close" bind:tap="closeDaoJu" src="../../images/chengzi/icon-close.png" mode="widthFix"></image>
                    <view class="flex-view" style="width: 100%;position: absolute;bottom: 0;">
                        <view bind:tap="clickDaoJuTab" data-type="{{item.type}}" wx:for="{{daoJuTab}}" wx:key="index" style="width: 33%">
                            <view class="daoJuTitle-tab font-30 font-bold color-ffffff" style="">{{item.text}}</view>
                            <view class="daoJuTitle-tab-border" style="{{item.type===daoJuNowType?'background-color: #ffae1a;':''}}"></view>
                        </view>
                    </view>
                </view>
                <scroll-view wx:if="{{daoJuNowType===0}}" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                    <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{allPropsList}}" wx:key="index" bind:tap="openBuyDaoJu" data-index="{{index}}" class="daoJuBox" style="background:url('../../images/chengzi/ban.png') 0 0/100% 100%">
<!--                            <image src="../../images/chengzi/shiyongzhong.png" class="daoJuBox-typeImg"></image>-->
                            <view class="daoJuBox-title font-28 font-bold color-ffffff">{{item.name}}</view>
                            <image class="daoJuBox-img" src="{{item.img}}"></image>
                            <view class="daoJuBox-btn font-28" style="background:url('../../images/chengzi/btn-quxiazai.png') 0 0/100% 100%;">
                                {{item.money}}元/天
                            </view>
                        </view>
                    </view>
                </scroll-view>
                <scroll-view wx:if="{{daoJuNowType===1}}" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                   <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{myPropsList}}" wx:key="index" class="daoJuBox" style="background:url('../../images/chengzi/ban.png') 0 0/100% 100%">
                            <image src="../../images/chengzi/shiyongzhong.png" class="daoJuBox-typeImg"></image>
                            <view class="daoJuBox-title font-28 font-bold color-ffffff">{{item.name}}</view>
                            <image class="daoJuBox-img" src="{{item.img}}"></image>
                            <view class="daoJuBox-btn font-28" style="background:url('../../images/chengzi/btn-quxiazai.png') 0 0/100% 100%;">
                                {{item.propsLaveTimeText}}到期
                            </view>
                        </view>
                    </view>
                </scroll-view>
                <scroll-view wx:if="{{daoJuNowType===2}}" bind:scrolltolower="getMyPropsOrderList" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                    <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{myPropsOrderList}}" wx:key="index" style="width: 100%;padding:20rpx 30rpx;background: #FEFDF8;border: 2rpx solid #23B854;border-radius: 30rpx;margin-bottom: 20rpx;">
                            <view class="flex-view font-26 color-898888" style="justify-content: space-between;padding: 10rpx 0;border-bottom: 1rpx solid #EBE7E7">
                                <view>订单号:{{item.orderNumber}}</view>
                                <view>下单时间:{{item.createTimeText}}</view>
                            </view>
                            <view class="flex-view" style="justify-content: space-between;padding: 10rpx 0;">
                                <view class="flex-view">
                                    <image style="width: 105rpx;height: 105rpx;display: block;margin-right: 18rpx;" src="{{item.img}}"></image>
                                    <view>
                                        <view class="font-30 font-bold color-302424">{{item.name}}</view>
                                        <view class="font-26 color-C0C0C0">x{{item.buyAmount}}</view>
                                    </view>
                                </view>
                                <view>
                                    <text class="font-20 color-302424">￥</text>
                                    <text class="font-36 color-302424">{{item.buyAllMoney}}</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </scroll-view>
            </view>
        </view>
        <!--集肥料-->
        <view class="bottom-window-shadow" style="{{jiFeiLiaoShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window">
                <view class="bottom-window-title">
                    <image class="bottom-window-title-icon" src="../../images/chengzi/jifeiliao-title.png" mode="widthFix"></image>
                    <image class="bottom-window-title-bg" src="../../images/chengzi/title1.png" mode="widthFix"></image>
                    <text class="bottom-window-title-text shouhuo-title font-40 font-bold color-ffffff">集肥料</text>
                    <image class="bottom-window-title-close" bind:tap="closeJiFeiLiao" src="../../images/chengzi/icon-close.png" mode="widthFix"></image>
                </view>
                <view class="bottom-window-content">
                    <view class="bottom-window-content-list" style="padding: 0">
                        <scroll-view style="height: 60vh;border-radius: 30rpx 30rpx 0px 0px;box-sizing: border-box;" scroll-y="true">
                            <view class="flex-view" style="padding: 40rpx 30rpx;border-radius: 30rpx 30rpx 0px 0px;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="../../images/chengzi/icon-shiping.png"></image>
                                    <view style="width: 285rpx;">
                                        <view class="font-30 font-bold color-302424">看视频领肥料({{watchAmount}}/{{maxAmount}})</view>
                                        <view class="font-24 color-C0C0C0" style="line-height: 25rpx">每看一段视频，可以领取500g肥料</view>
                                    </view>
                                </view>
                                <view class="dialog-btn1" style="width: 174rpx;height: 82rpx;line-height:72rpx;background:url('../../images/chengzi/btn-quguangkan.png') 0 0/100% 100%;">
                                    去观看
                                </view>
                            </view>
                            <view class="flex-view" style="padding: 40rpx 30rpx;border-radius: 30rpx 30rpx 0px 0px;background:#FFFAEA;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="../../images/chengzi/icon-xiazai.png"></image>
                                    <view style="width: 285rpx;">
                                        <view class="font-30 font-bold color-302424">下载柚子</view>
                                        <view class="font-24 color-C0C0C0" style="line-height: 25rpx">下载柚子APP，获得更多肥料</view>
                                    </view>
                                </view>
                                <view class="dialog-btn1" style="width: 174rpx;height: 82rpx;line-height:72rpx;background:url('../../images/chengzi/btn-quguangkan.png') 0 0/100% 100%;">
                                    去下载
                                </view>
                            </view>
                        </scroll-view>
                    </view>
                </view>
            </view>
        </view>
        <!--收获-->
        <view class="bottom-window-shadow" style="{{shouhuoShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window">
                <view class="bottom-window-title">
                    <image class="bottom-window-title-icon" src="../../images/chengzi/shouhuo-title.png" mode="widthFix"></image>
                    <image class="bottom-window-title-bg" src="../../images/chengzi/title1.png" mode="widthFix"></image>
                    <text class="bottom-window-title-text shouhuo-title font-40 font-bold color-ffffff">收获</text>
                    <image class="bottom-window-title-close" bind:tap="closeShouHuo" src="../../images/chengzi/icon-close.png" mode="widthFix"></image>
                </view>
                <view class="bottom-window-content">
                    <view style="width: 100%;padding:30rpx;box-sizing:border-box;background: #FEFDF8;box-shadow: 0rpx 10rpx 0rpx 0rpx rgba(24, 173, 73, 0.42);border-radius: 30rpx;margin-bottom: 21rpx">
                        <view class="font-24 color-898888">当前橙子数</view>
                        <view class="font-50 color-FE963A">{{nowOrange}}</view>
                        <view class="flex-view" style="justify-content: space-between;padding-bottom: 20rpx;">
                            <view style="width: 50%;">
                                <view class="font-24 color-898888">累计收入橙子</view>
                                <view class="font-34 color-302424">{{incomeOrange}}</view>
                            </view>
                            <view style="width: 50%;">
                                <view class="font-24 color-898888">累计支出橙子</view>
                                <view class="font-34 color-302424">{{expendOrange}}</view>
                            </view>
                        </view>
                        <view class="flex-view" style="justify-content: space-between;">
                            <image bind:tap="openSongChengZi" style="width: 278rpx;height: 90rpx;" src="../../images/chengzi/icon-songchengzi1.png" mode="widthFix"></image>
                            <image bind:tap="goToMall" style="width: 278rpx;height: 90rpx;" src="../../images/chengzi/icon-chengzishangcheng.png" mode="widthFix"></image>
                        </view>
                    </view>
                    <view class="bottom-window-content-list">
                        <view class="bottom-window-content-list-title">
                            <text class="font-30 color-302424 font-bold">橙子流水</text>
                        </view>
                        <scroll-view bindscrolltolower="getChengZiList" class="bottom-window-content-list-box" scroll-y="true">
                            <view wx:for="{{chengZiList}}" wx:key="index" class="flex-view" style="padding: 20rpx 0;border-bottom: 1rpx solid #EBE7E7;justify-content: space-between;">
                                <view class="flex-view">
                                    <image wx:if="{{item.orangeType===2||item.orangeType===3}}" style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="{{item.avatar}}"></image>
                                    <image wx:else style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="../../images/chengzi/houdecengzitouxiang.png"></image>
                                    <view>
<!--                                        orangeAmount:0,//橙子数量-->
<!--                                        orangeType:'',//橙子使用/获取途径(1获得橙子，2收到好友赠送，3送给好友橙子，4商城兑换)-->
<!--                                        avatar:'',//头像-->
<!--                                        friendId:'',//赠送/送给的好友id-->
<!--                                        nickName:'',//用户昵称-->
                                        <view class="font-30 color-302424">{{item.orangeTypeText}}</view>
                                        <view class="font-26 color-C0C0C0">{{item.createTimeText}}</view>
                                    </view>
                                </view>
                                <text class="font-40 font-bold {{item.orangeAmount>0?'color-FE963A':'color-ADAAAA'}}">{{item.orangeAmount}}</text>
                            </view>
                        </scroll-view>
                    </view>
                </view>
            </view>
        </view>
        <!--送橙子列表弹框-->
        <view wx:if="{{songChengZiShow}}" class="dialog-style">
            <view class="dialog-window">
                <view class="dialog-window-close">
                    <image bind:tap="closeSongChengZi" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/green.png') 0 0/100%;">
                    <image class="dialog-window-title" src="../../images/chengzi/icon-1.png"></image>
                    <view class="dialog-window-title-text font-36 font-bold color-ffffff">送橙子</view>
                    <view class="dialog-window-content">
                        <view class="flex-view" style="width: 508rpx;height: 80rpx;background: #FFF5D4;border-radius: 40rpx;padding: 0 40rpx;box-sizing: border-box;margin-bottom: 16rpx;">
                            <input class="font-28 color-FFAE1A" name="friendPhone" type="number" bindinput="bindFriendPhoneInput" placeholder="请输入好友橙子账号" value="{{friendPhone}}" />
                        </view>
                        <view class="flex-view" style="width: 508rpx;height: 80rpx;background: #FFF5D4;border-radius: 40rpx;padding: 0 40rpx;box-sizing: border-box;margin-bottom: 16rpx;">
                            <image style="width: 36rpx;height: 36rpx;margin-right: 11rpx;" src="../../images/chengzi/icon2.png"></image>
                            <input class="font-28 color-FFAE1A" name="friendNumber" type="number" bindinput="bindFriendNumberInput" placeholder="请输入橙子数量" value="{{friendNumber}}" />
                        </view>
                        <view class="font-24 color-FE963A" style="text-align: right;">{{nowOrange}}个橙子可送</view>
                        <view bind:tap="clickNext" class="dialog-btn" style="width: 280rpx;height:86rpx;line-height:70rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            下一步
                        </view>
                        <scroll-view style="max-height: 30vh;min-height: 15vh;" scroll-y="true">
                            <view wx:for="{{friendList}}" wx:key="index" class="flex-view" style="padding: 15rpx 0;border-bottom: 1rpx solid #EBE7E7;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="{{item.avatar}}"></image>
                                    <view>
                                        <view class="font-30 color-302424">{{item.nickName}}</view>
                                        <view class="font-26 color-C0C0C0">{{item.phone}}</view>
                                    </view>
                                </view>
                                <image bind:tap="clickFriendList" data-phone="{{item.phone}}" style="width: 140rpx;height:60rpx" src="../../images/chengzi/btn-songchengzi.png"></image>
                            </view>
                        </scroll-view>
                    </view>
                </view>
            </view>
        </view>
        <!--设置密码框 setPasswordType  0关闭  1设置密码  2输入密码 3忘记密码输入验证码  4重置密码-->
        <view wx:if="{{setPasswordType}}" class="dialog-style">
            <view class="dialog-window">
                <view class="dialog-window-close">
                    <image bind:tap="closeSetPassword" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/green.png') 0 0/100%;">
                    <image class="dialog-window-title" src="../../images/chengzi/icon-1.png"></image>
                    <view wx:if="{{setPasswordType === 1}}" class="dialog-window-title-text font-36 font-bold color-ffffff">设置橙子密码</view>
                    <view wx:if="{{setPasswordType === 2}}"  class="dialog-window-title-text font-36 font-bold color-ffffff">输入橙子密码</view>
                    <view wx:if="{{setPasswordType === 3}}"  class="dialog-window-title-text font-36 font-bold color-ffffff">忘记橙子密码</view>
                    <view wx:if="{{setPasswordType === 4}}"  class="dialog-window-title-text font-36 font-bold color-ffffff">重置橙子密码</view>
                    <view class="dialog-window-content">
                        <view wx:if="{{setPasswordType === 3}}" class="flex-view" style="justify-content: space-between;margin-bottom: 39rpx">
                            <view class="font-30 color-302424">手机号</view>
                            <view class="flex-view" style="justify-content:space-between;width: 402rpx;height: 80rpx;background: #FFF5D4;border-radius: 40rpx;padding: 0 20rpx;box-sizing: border-box">
                                <view class="font-32 color-302424">+86</view>
                                <text class="font-28 color-FFAE1A">17638172927</text>
                                <view wx:if="{{second>=60}}" bind:tap="clickSendCode" class="font-24 color-ffffff" style="width: 90rpx;height: 50rpx;line-height:50rpx;text-align:center;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                    发送
                                </view>
                                <view wx:else class="font-24 color-ffffff" style="width: 90rpx;height: 50rpx;line-height:50rpx;text-align:center;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                    {{second}}s
                                </view>
                            </view>
                        </view>
                        <view style="width: 100%;margin-bottom: 29rpx">
                            <view wx:if="{{setPasswordType === 1||setPasswordType === 4}}" style="width: 100%;margin-bottom: 20rpx" class="font-30 color-302424">设置密码</view>
                            <view wx:if="{{setPasswordType === 2}}" style="width: 100%;margin-bottom: 20rpx" class="font-30 color-302424">请输入橙子密码</view>
                            <view wx:if="{{setPasswordType === 3}}" style="width: 100%;margin-bottom: 20rpx" class="font-30 color-302424">请输入手机验证码</view>
                            <six-password
                                    bindvalueSix="valueSix"
                                    input_value="{{inputData.input_value}}"
                                    value_length="{{inputData.value_length}}"
                                    isNext="{{inputData.isNext}}"
                                    get_focus="{{inputData.get_focus}}"
                                    focus_class="{{inputData.focus_class}}"
                                    value_num="{{inputData.value_num}}"
                                    height="{{inputData.height}}"
                                    width="{{inputData.width}}"
                                    see="{{inputData.see}}"
                                    interval="{{inputData.interval}}">
                            </six-password>
                        </view>
                        <view wx:if="{{setPasswordType === 1||setPasswordType === 4}}" style="width: 100%;margin-bottom: 29rpx">
                            <view style="width: 100%;margin-bottom: 20rpx" class="font-30 color-302424">确认密码</view>
                            <six-password
                                    bindvalueSix="valueSix1"
                                    input_value="{{inputData1.input_value}}"
                                    value_length="{{inputData1.value_length}}"
                                    isNext="{{inputData1.isNext}}"
                                    get_focus="{{inputData1.get_focus}}"
                                    focus_class="{{inputData1.focus_class}}"
                                    value_num="{{inputData1.value_num}}"
                                    height="{{inputData1.height}}"
                                    width="{{inputData1.width}}"
                                    see="{{inputData1.see}}"
                                    interval="{{inputData1.interval}}">
                            </six-password>
                        </view>
                        <view bind:tap="clickSetPassword" wx:if="{{setPasswordType === 1}}" class="dialog-btn" style="width: 280rpx;height: 100rpx;line-height:90rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            确定
                        </view>
                        <view bind:tap="giveFriend" wx:if="{{setPasswordType === 2}}" class="dialog-btn" style="width: 280rpx;height: 100rpx;line-height:90rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            确定
                        </view>
                        <view bind:tap="clickCheckCode" wx:if="{{setPasswordType === 3}}" class="dialog-btn" style="width: 280rpx;height: 100rpx;line-height:90rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            下一步
                        </view>
                        <view bind:tap="clickSetPassword" wx:if="{{setPasswordType === 4}}" class="dialog-btn" style="width: 280rpx;height: 100rpx;line-height:90rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            确定
                        </view>
                    </view>
                    <view wx:if="{{setPasswordType === 2}}" style="height: 56rpx;margin: 30rpx 0 0 0">
                        <view bind:tap="openSetPassword3" class="font-30 color-ffffff" style="width: 230rpx;height: 56rpx;background-color: #29BE52;border-radius: 28rpx;line-height: 56rpx;text-align: center;float: right">
                            忘记橙子密码?
                        </view>
                    </view>
                </view>
            </view>
        </view>
        <!--去设置密码弹框-->
        <view wx:if="{{tipsPasswordShow}}" class="dialog-style">
            <view class="dialog-window">
                <view style="position: absolute;top: 47rpx;right: 16rpx;">
                    <image style="width: 47rpx;height: 47rpx;" bind:tap="closeTipsPassword" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/daojuxiangqing-bg.png') 0 0/100%;">
                    <view class="dialog-window-content" style="margin-top: 150rpx;">
                        <view class="font-40 font-bold color-FE963A" style="padding: 20rpx 0;text-align: center">你还未设置橙子密码~</view>
                        <view class="font-32 font-bold color-302424" style="margin-bottom: 50rpx;text-align: center">快去设置吧~</view>
                        <view class="flex-view" style="justify-content: space-between">
                            <view bind:tap="closeTipsPassword" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                取消
                            </view>
                            <view bind:tap="goToSetPassword" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                立即设置
                            </view>
                        </view>

                    </view>
                </view>
            </view>
        </view>
        <!--买道具弹框-->
        <view wx:if="{{buyDaoJuShow}}" class="dialog-style">
            <view class="dialog-window">
                <view style="position: absolute;top: 47rpx;right: 16rpx;">
                    <image style="width: 47rpx;height: 47rpx;" bind:tap="closeBuyDaoJu" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/daojuxiangqing-bg.png') 0 0/100%;">
                    <view class="dialog-window-content" style="margin-top: 150rpx;">
                        <image style="width: 216rpx;height: 216rpx;display: block;margin:0 auto;" src="{{allPropsList[nowShowPropsIndex].img}}"></image>
                        <view class="font-36 font-bold color-302424" style="padding: 30rpx 0;text-align: center">{{allPropsList[nowShowPropsIndex].name}}</view>
                        <view class="font-28 color-ADAAAA" style="">{{allPropsList[nowShowPropsIndex].propsDesc}}</view>
                        <view class="font-30 font-bold color-FE963A" style="padding: 30rpx 0;text-align: center">{{allPropsList[nowShowPropsIndex].money}}元/天</view>
                        <view class="flex-view" style="width: 360rpx;height: 80rpx;border: 1rpx solid #ADAAAA;border-radius: 20rpx;margin: 0 auto;margin-bottom: 40rpx">
                            <view bind:tap="lessBuyCount" style="width: 93rpx;height: 80rpx;line-height:80rpx;text-align: center;">-</view>
                            <view style="width: 169rpx;height: 80rpx;line-height:80rpx;text-align: center;border-left: 1rpx solid #ADAAAA;border-right: 1rpx solid #ADAAAA;">
                                {{buyCount}}
                            </view>
                            <view bind:tap="addBuyCount" style="width: 93rpx;height: 80rpx;line-height:80rpx;text-align: center;">+</view>
                        </view>
                        <view class="dialog-btn" style="width: 280rpx;height: 100rpx;line-height:90rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                            立即购买
                        </view>
                    </view>
                </view>
            </view>
        </view>
        <!--领取肥料时 没有可领取的肥料 弹框-->
        <view wx:if="{{noCanLingQuShow}}" class="dialog-style">
            <view class="dialog-window">
                <view style="position: absolute;top: 47rpx;right: 16rpx;">
                    <image style="width: 47rpx;height: 47rpx;" bind:tap="closeNoCanLingQu" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/daojuxiangqing-bg.png') 0 0/100%;">
                    <view class="dialog-window-content" style="margin-top: 150rpx;">
                        <view class="font-40 font-bold color-FE963A" style="padding: 20rpx 0;text-align: center">没有可领取的肥料啦！</view>
                        <view class="font-32 font-bold color-302424" style="margin-bottom: 50rpx;text-align: center">快去集肥料吧~~</view>
                        <view class="flex-view" style="justify-content: space-between">
                            <view bind:tap="clickFeiLiaoList" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                肥料记录
                            </view>
                            <view bind:tap="clickJiFeiLiao" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                集肥料
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
        <!--施肥时 肥料不足 弹框-->
        <view wx:if="{{noFeiLiaoShow}}" class="dialog-style">
            <view class="dialog-window">
                <view style="position: absolute;top: 47rpx;right: 16rpx;">
                    <image style="width: 47rpx;height: 47rpx;" bind:tap="closeNoFeiLiaoShow" src="../../images/chengzi/icon-close.png"></image>
                </view>
                <view class="dialog-window-box" style="background: url('../../images/chengzi/daojuxiangqing-bg.png') 0 0/100%;">
                    <view class="dialog-window-content" style="margin-top: 150rpx;">
                        <view class="font-40 font-bold color-FE963A" style="padding: 20rpx 0;text-align: center">没有可领取的肥料啦！</view>
                        <view class="font-32 font-bold color-302424" style="margin-bottom: 50rpx;text-align: center">快去集肥料吧~~</view>
                        <view class="flex-view" style="justify-content: space-between">
                            <view bind:tap="clickShiFeiList" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                施肥记录
                            </view>
                            <view bind:tap="clickJiFeiLiao" class="dialog-btn" style="width: 240rpx;height: 80rpx;line-height:62rpx;background:url('../../images/chengzi/icon-btnBg.png') 0 0/100% 100%;">
                                集肥料
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>
