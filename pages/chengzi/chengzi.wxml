                            <!--分享好友-->
                            <view class="flex-view" style="padding: 20rpx 0;border-radius: 30rpx 30rpx 0px 0px;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="http://img.laishida.cn/images/chengzi/icon-fenxiang.png"></image>
                                    <view style="width: 285rpx;">
                                        <view class="font-30 font-bold color-302424">分享微信好友({{shareFriend}}/{{shareFriendMax}})</view>
                                        <view class="font-24 color-C0C0C0" style="line-height: 25rpx">分享微信好友获得肥料</view>
                                    </view>
                                </view>
                                <view wx:if="{{shareFriend<shareFriendMax}}"  style="width: 160rpx;height: 60rpx;" class="btn-box">
                                    <button open-type="share" style="width: 160rpx;background-color: #00000000;height: 60rpx;border:0;margin: 0;padding: 0;">
                                        <view style="width: 160rpx;height: 60rpx;" class="btn-box">
                                            <image class="btn-box-img" src="http://img.laishida.cn/images/chengzi/btn-bg.png"></image>
                                            <view class="btn-box-text font-28 color-ffffff">去分享</view>
                                        </view>
                                    </button>
                                </view>
                                <view wx:if="{{shareFriend>=shareFriendMax && shareFriendMuck!=0}}">
                                    <view class="flex-view" style="justify-content: center;">
                                        <image style="width:30rpx;height:40rpx;margin-right:10rpx;display:block;" src="http://img.laishida.cn/images/chengzi/feiliao.png"></image>
                                        <text class="font-26 color-FCA02F">x{{shareFriendMuck}}g</text>
                                    </view>
                                    <view bind:tap="lingQuFeiLiao" data-type="4" style="width: 160rpx;height: 60rpx;" class="btn-box">
                                        <image class="btn-box-img" src="http://img.laishida.cn/images/chengzi/btn-bg.png"></image>
                                        <view class="btn-box-text font-28 color-ffffff">领取</view>
                                    </view>
                                </view>
                                <view wx:if="{{shareFriend>=shareFriendMax && shareFriendMuck==0}}" style="width: 160rpx;height: 60rpx;" class="btn-box">
                                    <image class="btn-box-img" src="http://img.laishida.cn/images/chengzi/btn-bg.png"></image>
