        <!--果树部分-->
        <view style="width: 593rpx;height: 287rpx;position: absolute;top: 50%;left: 50%;transform: translate(-50%,75%)">
            <image style="width: 100%" src="http://img.laishida.cn/images/chengzi/keng.png" mode="widthFix"></image>
            <image bind:tap="clickShu" class="{{shuClassName}}" style="position: absolute;bottom: 88rpx;width: 100%;left: 0%;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/{{notChargeOrangeAmount&&isMyTree?'2':'1'}}-{{treeRank===1?treeRank:treeRank-1}}.png"></image>
            <view style="text-align: center;width: 150rpx;position: absolute;top: 54rpx;left: 78rpx;transform:rotateZ(-5deg);line-height: 26rpx;">
                <view class="font-20 color-ffeda6">{{signName}}</view>
                <view class="font-20 color-ffeda6">的杨桃乐园</view>
            </view>
            <image wx:for="{{feiLiaoBing}}" wx:key="index" class="{{item}}" style="width: 50rpx;height: 50rpx;position: absolute;bottom: 140rpx;right: 120rpx;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/feiliaoli.png"></image>
            <view wx:if="{{isMyTree}}" class="{{feiLiaoDaiClass}}" bind:tap="clickFeiLiaoBao" style="position: absolute;bottom: 111rpx;width: 125rpx;height:125rpx;right: 82rpx;">
                <image class="{{notChargeMuckAmount>0?'fei-liao-bao':''}}" style="width: 100%;height: auto" mode="widthFix" src="http://img.laishida.cn/images/chengzi/icon-feiliao.png"></image>
                <view class="{{notChargeMuckAmount>0?'fei-liao-bao':''}}"  style="position: absolute;width: 100%;bottom: 14rpx;text-align: center;line-height: 30rpx;">
                    <text class="font-20 color-8F491B">
                        肥料
                    </text>
                    <text class="font-30 font-bold color-8F491B">{{notChargeMuckAmount}}</text>
                </view>
                <image wx:if="{{notChargeMuckAmount}}" class="can-Ling-qu" src="http://img.laishida.cn/images/chengzi/qipao.png" mode="widthFix"></image>
            </view>
            <view wx:if="{{!isMyTree}}" class="{{feiLiaoDaiClass}}" bind:tap="clickTou" style="position: absolute;bottom: 111rpx;width: 125rpx;height:125rpx;right: 82rpx;">
                <image class="{{canStealCount>0?'fei-liao-bao':''}}" style="width: 100%;height: auto" mode="widthFix" src="http://img.laishida.cn/images/chengzi/icon-feiliao.png"></image>
                <view class="{{canStealCount>0?'fei-liao-bao':''}}" style="position: absolute;width: 100%;bottom: 14rpx;text-align: center;line-height: 30rpx;">
                    <text class="font-20 color-8F491B">
                        肥料
                    </text>
                    <text class="font-30 font-bold color-8F491B">{{canStealCount}}</text>
                </view>
                <image class="can-Ling-qu" src="http://img.laishida.cn/images/chengzi/tou-qipao.png" mode="widthFix"></image>
            </view>
            <image style="position: absolute;bottom: 45rpx;width: 68rpx;height:auto;right: 98rpx" mode="widthFix" src="http://img.laishida.cn/images/chengzi/huaduo.gif"></image>
            <view wx:if="{{isMyTree}}" style="position:absolute;bottom:0;left:50%;transform:translate(-50%,0);width: 300rpx;height: 88rpx;text-align:center;">
                <view style="width: 300rpx;border: 3rpx solid #FDEB9F;border-radius: 18rpx;background-color: #8C584A;padding: 3rpx;">
                    <view style="width: {{progressWidth}}%" class="progress"></view>
                </view>
                <text wx:if="{{treeRank<10}}" style="white-space: nowrap;" class="font-24 color-ffffff">再施肥{{muckAmount}}g就长成{{treeName}}啦</text>
                <text wx:else style="white-space: nowrap;" class="font-24 color-ffffff">您已经是最高级了</text>
            </view>
            <view wx:if="{{isMyTree}}" class="{{shiFeiBtnClass}}" bind:tap="" style="position: absolute;bottom: -10vh;left:114rpx;width: 365rpx;height:125rpx;">
                <image wx:if="{{shiFeiGifShow}}" style="width: 100%;position: absolute;bottom: 80rpx;left: 0;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/shifei.gif"></image>
                <image style="width: 100%" mode="widthFix" src="http://img.laishida.cn/images/chengzi/btnshifei-bg.png"></image>
                <view bind:tap="clickShiFeiBtn" style="position: absolute;top:0;left:0;width: 100%;text-align: center;padding-top: 22rpx;line-height: 40rpx;">
                    <view class="font-36 font-bold color-8F491B">施肥</view>
                    <view class="font-26 font-bold color-BC5D1E">我的肥料 {{notFertilizeAmount}}</view>
                </view>
            </view>
            <view wx:if="{{!isMyTree}}" class="{{shiFeiBtnClass}}" bind:tap="" style="position: absolute;bottom: -10vh;left:114rpx;width: 365rpx;height:125rpx;">
                <image style="width: 100%" mode="widthFix" src="http://img.laishida.cn/images/chengzi/btnshifei-bg.png"></image>
                <view bind:tap="goToMyTree" style="position: absolute;top:0;left:0;width: 100%;text-align: center;padding-top: 22rpx;line-height: 40rpx;">
                    <view class="font-36 font-bold color-8F491B">我的庄园</view>
                    <view class="font-26 font-bold color-BC5D1E">我的肥料 {{notFertilizeAmount}}</view>
                </view>
            </view>
            <view wx:if="{{notChargeOrangeAmount&&isMyTree}}" bind:tap="clickZaiQu" class="font-20 color-ffffff can-zhai-qu">
                <view class="flex-view" style="justify-content: center">
                    <image style="width: 24rpx;height: 24rpx;" src="http://img.laishida.cn/images/chengzi/chengZiTitle.png"></image>
                    <view class="font-26 font-bold color-ffffff right-list-text1" style="padding: 0 14rpx">可摘取</view>
                    <image style="width: 24rpx;height: 24rpx;" src="http://img.laishida.cn/images/chengzi/chengZiTitle.png"></image>
                </view>
                <view class="font-24 font-bold color-ffffff right-list-text1">{{notChargeOrangeAmount}}个杨桃</view>
            </view>
            <image  wx:for="{{shouHuoList}}" wx:key="index" class="{{item}}" style="width: 50rpx;height: 50rpx;position: absolute;bottom: 500rpx;left: 300rpx;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/icon-1.png"></image>
        </view>
        <!--杨桃池子-->
        <view style="position: absolute;top:10vh;left:50%;transform:translate(-50%,0);text-align: center;">
            <view style="width: 500rpx;height: 250rpx;position: relative;transform: translate(0 , -30rpx)">
                <image style="width: 500rpx;height: 250rpx;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/yangtaoguoyuanzhutu.png"></image>
                <view style="position: absolute;bottom:40rpx;left:0;width: 100%;text-align: center;padding-top: 38rpx;line-height: 46rpx">
                    <view class="font-34 font-bold color-ffffff leyuan-title1">今日杨桃数</view>
                    <view class="font-34 font-bold color-ffffff leyuan-title1">{{todayNumber}}个</view>
                </view>
                <view style="position: absolute;bottom:-20rpx;width:100%" class="muban-text font-28 font-bold color-ffffff">施肥越多获得的杨桃越多</view>
            </view>
        </view>
        <view wx:if="{{isMyTree}}" class="right-list flex-view" style="bottom: 40rpx;">
            <view class="{{shouHuoBtnClass}}" bind:tap="openShouHuo">
                <image src="http://img.laishida.cn/images/chengzi/btn-shouhuo.png" mode="widthFix"></image>
                <text class="font-26 right-list-text1">收获</text>
            </view>
            <view bind:tap="openTouFeiLiao">
                <image class="guang-huan" src="http://img.laishida.cn/images/chengzi/guanghuan.png" mode="widthFix"></image>
                <image src="http://img.laishida.cn/images/chengzi/btn-zhaofeiliao.png" mode="widthFix"></image>
                <text class="font-26 right-list-text2">找肥料</text>
<!--                <view class="font-20 color-ffffff red-tips" style="background: url('http://img.laishida.cn/images/chengzi/redTips.png') 0 0/100%">可偷</view>-->
            </view>
            <view bind:tap="getAllPropsList">
                <image src="http://img.laishida.cn/images/chengzi/btn-daoju.png" mode="widthFix"></image>
                <text class="font-26 right-list-text3">道具</text>
            </view>
            <view bind:tap="openJiFeiLiao">
                <image src="http://img.laishida.cn/images/chengzi/btn-jifenliao.png" mode="widthFix"></image>
                <text class="font-26 right-list-text4">集肥料</text>
            </view>
        </view>
        <!--杨桃商城-->
        <image bind:tap="goToMall" style="width:300rpx;position: absolute;top: 35vh;right: -50rpx;" mode="widthFix" src="http://img.laishida.cn/images/chengzi/shangcheng.png"></image>
        <!--    下方滑出弹框-->
        <!--找肥料-->
        <view class="bottom-window-shadow" style="{{touFeiLiaoShow?'top: 0;':'top:100%;'}}">
            <view class="bottom-window" style="{{touFeiLiaoShow?'bottom: 0;':'bottom:-100vh;'}}">
                <view class="bottom-window-title">
                    <image class="bottom-window-title-icon" src="http://img.laishida.cn/images/chengzi/zhaofeiliaotitle.png" mode="widthFix"></image>
                    <image class="bottom-window-title-close" bind:tap="closeTouFeiLiao" src="http://img.laishida.cn/images/chengzi/icon-close.png" mode="widthFix"></image>
                </view>
                <view class="bottom-window-content">
                    <view class="bottom-window-content-list" style="padding: 25rpx;">
                        <view class="flex-view" style="background: #FFD26440;border-radius: 50rpx;box-shadow: 0px 10px 18px 2px rgba(0, 0, 0, 0.1);padding:25rpx;justify-content: space-between;">
                            <view class="flex-view">
                                <image style="width: 106rpx;height: 106rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="http://img.laishida.cn/images/chengzi/zhaoqu.png"></image>
                                <view>
                                    <view class="font-36 color-FE1943">防偷道具</view>
                                    <view class="font-24 color-C0C0C0">防止肥料被偷</view>
                                </view>
                            </view>
                            <view bind:tap="clickGoBuy" style="width: 173rpx;height: 81rpx;" class="btn-box">
                                <image class="btn-box-img" src="http://img.laishida.cn/images/chengzi/btn-bg.png"></image>
                                <view class="btn-box-text font-34 color-ffffff">去购买</view>
                            </view>
                        </view>
                        <scroll-view bindscrolltolower="getCanStealList" style="height: 50vh;box-sizing: border-box;" scroll-y="true">
                            <view wx:for="{{canStealList}}" wx:key="index" class="flex-view" style="padding: 20rpx 0;border-bottom: 1rpx solid #EBE7E7;justify-content: space-between;">
                                <view class="flex-view">
                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="{{item.avatar}}"></image>
                                    <view>
                                        <view class="font-30 color-302424">{{item.nickName}}</view>
                                        <view class="font-26 color-C0C0C0">{{item.phone}}</view>
                                    </view>
                                </view>
                                <image wx:if="{{item.id}}" bind:tap="goToSteal" data-i="{{index}}" src="http://img.laishida.cn/images/chengzi/btn-zhaofeiliao.png" style="width: 80rpx;height: 84rpx"></image>
                            </view>
                        </scroll-view>
                    </view>
                </view>
            </view>
        </view>
        <!--道具-->
        <view class="bottom-window-shadow" style="{{daoJuShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window" style="{{daoJuShow?'bottom: 0;':'bottom:-100vh;'}}">
                <view class="bottom-window-title" style="height: 156rpx">
                    <image class="bottom-window-title-icon" style="top:-122rpx;" src="http://img.laishida.cn/images/chengzi/daojutitle.png" mode="widthFix"></image>
                    <image class="bottom-window-title-close" style="top:0" bind:tap="closeDaoJu" src="http://img.laishida.cn/images/chengzi/icon-close.png" mode="widthFix"></image>
                    <view class="flex-view" style="width: 100%;position: absolute;bottom: 0;background-color: #d5ec96;">
                        <view bind:tap="clickDaoJuTab" data-type="{{item.type}}" wx:for="{{daoJuTab}}" wx:key="index" style="width: 33%">
                            <view class="daoJuTitle-tab font-30 font-bold color-ffffff" style="">{{item.text}}</view>
                            <view class="daoJuTitle-tab-border" style="{{item.type===daoJuNowType?'background-color: #ffae1a;':''}}"></view>
                        </view>
                    </view>
                </view>
                <scroll-view wx:if="{{daoJuNowType===0}}" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                    <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{allPropsList}}" wx:key="index" bind:tap="openBuyDaoJu" data-index="{{index}}" class="daoJuBox">
<!--                            <image src="http://img.laishida.cn/images/chengzi/shiyongzhong.png" class="daoJuBox-typeImg"></image>-->
                            <view class="daoJuBox-title font-28 font-bold color-ffffff">{{item.name}}</view>
                            <image class="daoJuBox-img" src="{{item.img}}"></image>
                            <view bind:tap="clickGoBuy" style="width: 173rpx;height: 60rpx;margin:0 auto;" class="btn-box">
                                <image class="btn-box-img" src="http://img.laishida.cn/images/chengzi/btn-bg.png"></image>
                                <view class="btn-box-text font-28 color-ffffff">{{item.money}}元/天</view>
                            </view>


                        </view>
                    </view>
                </scroll-view>
                <scroll-view wx:if="{{daoJuNowType===1}}" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                   <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{myPropsList}}" wx:key="index" class="daoJuBox">
                            <image src="http://img.laishida.cn/images/chengzi/shiyongzhong.png" class="daoJuBox-typeImg"></image>
                            <view class="daoJuBox-title font-28 font-bold color-ffffff">{{item.name}}</view>
                            <image class="daoJuBox-img" src="{{item.img}}"></image>
                            <view class="daoJuBox-btn font-28">
                                {{item.propsLaveTimeText}}到期
                            </view>
                        </view>
                    </view>
                </scroll-view>
                <scroll-view wx:if="{{daoJuNowType===2}}" bind:scrolltolower="getMyPropsOrderList" style="height: 60vh;padding: 22rpx 16rpx;box-sizing: border-box;background-color: #F5FFEE;" scroll-y="true">
                    <view style="width: 100%;display: flex;flex-wrap: wrap;">
                        <view wx:for="{{myPropsOrderList}}" wx:key="index" style="width: 100%;padding:20rpx 30rpx;background: #FEFDF8;border: 2rpx solid #23B854;border-radius: 30rpx;margin-bottom: 20rpx;">
                            <view class="flex-view font-26 color-898888" style="justify-content: space-between;padding: 10rpx 0;border-bottom: 1rpx solid #EBE7E7">
                                <view>订单号:{{item.orderNumber}}</view>
                                <view>下单时间:{{item.createTimeText}}</view>
                            </view>
                            <view class="flex-view" style="justify-content: space-between;padding: 10rpx 0;">
                                <view class="flex-view">
                                    <image style="width: 105rpx;height: 105rpx;display: block;margin-right: 18rpx;" src="{{item.img}}"></image>
                                    <view>
                                        <view class="font-30 font-bold color-302424">{{item.name}}</view>
                                        <view class="font-26 color-C0C0C0">x{{item.buyAmount}}</view>
                                    </view>
                                </view>
                                <view>
                                    <text class="font-20 color-302424">￥</text>
                                    <text class="font-36 color-302424">{{item.buyAllMoney}}</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </scroll-view>
            </view>
        </view>
        <!--集肥料-->
        <view class="bottom-window-shadow" style="{{jiFeiLiaoShow?'top: 0%;':'top: 100%;'}}">
            <view class="bottom-window" style="{{jiFeiLiaoShow?'bottom: 0;':'bottom:-100vh;'}}">
                <view class="bottom-window-title">
                    <image class="bottom-window-title-icon" src="http://img.laishida.cn/images/chengzi/jifeiliaotitle.png" mode="widthFix"></image>
                    <image class="bottom-window-title-close" bind:tap="closeJiFeiLiao" src="http://img.laishida.cn/images/chengzi/icon-close.png" mode="widthFix"></image>
                </view>
                <view class="bottom-window-content">
                    <view class="bottom-window-content-list" style="padding: 0">
                        <scroll-view style="height: 60vh;padding:29rpx 33rpx;border-radius: 60rpx 60rpx 0px 0px;box-sizing: border-box;" scroll-y="true">
