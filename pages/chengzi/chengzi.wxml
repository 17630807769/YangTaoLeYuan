<!--                                <view class="flex-view">-->
<!--                                    <image style="width: 100rpx;height: 100rpx;margin-right: 20rpx;border-radius: 50%;overflow: hidden" src="http://img.laishida.cn/images/chengzi/icon-vvip.png"></image>-->
