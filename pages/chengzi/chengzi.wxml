<!--pages/chengzi/chengzi.wxml-->
<view class="you-zi">
    <image class="bg-img" src="http://img.laishida.cn/images/chengzi/bg.png" mode="widthFix"></image>
    <view class="yun-ceng">
        <image class="yun-max" src="http://img.laishida.cn/images/chengzi/dayun.png"></image>
        <image class="yun-min" src="http://img.laishida.cn/images/chengzi/xiaoyun.png"></image>
    </view>
    <view class="content">
<!--        <view style="width: 40rpx;height: 70rpx;position: absolute;top: 50%;right: 100rpx;transform: translate(0%,250%)">-->
<!--            <image style="width: 40rpx;height: 70rpx;" src="http://img.laishida.cn/images/chengzi/fengche1.png"></image>-->
<!--            <image class="feng-che" style="width: 68rpx;height: 68rpx;position: absolute;top: -16rpx;left: -13rpx;" src="http://img.laishida.cn/images/chengzi/fengche2.png"></image>-->
