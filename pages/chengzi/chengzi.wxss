    30% {bottom: 236rpx;right: 120rpx;opacity: 1;z-index: 1}
    50% {opacity: 1}
    100% {bottom: -10.2vh;right: 200rpx;}
}
.fei-liao-kuai4{
    animation: fei-liao-kuai-dong4 2000ms linear ;
}
@keyframes fei-liao-kuai-dong4 {
    0% {bottom: 140rpx;right: 120rpx;}
    30% {bottom: 236rpx;right: 120rpx;opacity: 1;z-index: 1}
    50% {opacity: 1}
    100% {bottom: -10.2vh;right: 130rpx;}
}
.shu-hu-xi{
    animation: hu-xi 3000ms linear infinite;
}
@keyframes hu-xi {
    0%{
        transform: scale3d(1, 1, 1);
    }
    25%{
        transform: scale3d(1.01, 0.99, 1);
        transform-origin: 280rpx 500rpx;
    }
    75%{
        transform: scale3d(0.99, 1.01, 1);
        transform-origin: 280rpx 500rpx;
    }
    100%{
        transform: scale3d(1, 1, 1);
    }
}
.shu-q-tan{
    animation: q-tan 1000ms linear;
}
@keyframes q-tan {
    0%{
        transform: scale3d(1, 1, 1);
    }
    20%{
        transform: scale3d(1.01, 0.99, 1);
        transform-origin: 280rpx 500rpx;
    }
    40%{
        transform: scale3d(0.99, 1.01, 1);
        transform-origin: 280rpx 500rpx;
    }
    60%{
        transform: scale3d(1.01, 0.99, 1);
        transform-origin: 280rpx 500rpx;
    }
    80%{
        transform: scale3d(0.99, 1.01, 1);
        transform-origin: 280rpx 500rpx;
    }
    100%{
        transform: scale3d(1, 1, 1);
    }
}
.feng-che{
    animation: feng-che-dong 3000ms linear infinite;
}
@keyframes feng-che-dong {
    0%{
        transform: rotateZ(0deg);
    }
    100%{
        transform: rotateZ(-360deg);
    }
}
.can-Ling-qu{
    width: 98rpx;height: 46rpx;position: absolute;left: 14rpx;top: -50rpx;
    animation: can-Ling-qu-dong 3000ms linear infinite;
}
@keyframes can-Ling-qu-dong{
    0%{
        transform: translateY(0%) rotateY(0deg);
    }
    20%{
        transform: translateY(20%) rotateY(0deg);
    }
    40%{
        transform: translateY(0%) rotateY(0deg);
    }
    60%{
        transform: translateY(20%) rotateY(0deg);
    }
    80%{
        transform: translateY(0%) rotateY(0deg);
    }
    100%{
        transform: translateY(0%) rotateY(360deg);
    }
}
.can-zhai-qu{
    background: url('http://cz.h5.krjie.com//images/chengzi/kezhaichengziqipao.png') 0 0/100%;
    width: 242rpx;position: absolute;left: 175rpx;top: -500rpx;
    text-align: center;
    padding: 9rpx 0;
    box-sizing: border-box;
    animation: can-zhai-qu-dong 1000ms linear infinite;
}
.can-zhai-qu-text{
    -webkit-text-stroke: 1rpx #FF8B2F;
